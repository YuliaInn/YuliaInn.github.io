	month = nov,
	year = {2020},
	note = {Number: 11
Publisher: Multidisciplinary Digital Publishing Institute},
	keywords = {MDSCs, CD44, immune checkpoint, immune evasion, integrin, osteopontin, tumor-associated macrophage},
	pages = {3379},
	file = {Full Text PDF:/Users/yuliai/Zotero/storage/DELTL77P/Moorman et al. - 2020 - Osteopontin A Key Regulator of Tumor Progression .pdf:application/pdf;Snapshot:/Users/yuliai/Zotero/storage/5WF7DFWH/htm.html:text/html},

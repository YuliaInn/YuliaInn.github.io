	journal = {Cancer Immunology, Immunotherapy},
	author = {Sherwood, Anna M. and Emerson, Ryan O. and Scherer, Dominique and Habermann, Nina and Buck, Katharina and Staffa, Jürgen and Desmarais, Cindy and Halama, Niels and Jaeger, Dirk and Schirmacher, Peter and Herpel, Esther and Kloor, Matthias and Ulrich, Alexis and Schneider, Martin and Ulrich, Cornelia M. and Robins, Harlan},

@article{tieng_single_2020,
	title = {Single {Cell} {Transcriptome} in {Colorectal} {Cancer}—{Current} {Updates} on {Its} {Application} in {Metastasis}, {Chemoresistance} and the {Roles} of {Circulating} {Tumor} {Cells}},
	volume = {11},
	issn = {1663-9812},
	url = {https://www.frontiersin.org/articles/10.3389/fphar.2020.00135/full},
	doi = {10.3389/fphar.2020.00135},
	abstract = {Colorectal cancer (CRC) is among the most common cancer worldwide, a challenge for research, and a model for studying the molecular mechanisms involved in its development. Previously, bulk transcriptomics analyses were utilized to classify CRC based on its distinct molecular and clinicopathological features for prognosis and diagnosis of patients. The introduction of single-cell transcriptomics completely turned the table by enabling the examination of the expression levels of individual cancer cell within a single tumour. In this review, we highlighted the importance of these single-cell transcriptomics analyses as well as suggesting circulating tumour cells (CTCs) as the main focus of single-cell RNA sequencing. Characterization of these cells might reveal the intratumoral heterogeneity present in CRC while providing critical insights into cancer metastasis. To summarize, we believed the analysis of gene expression patterns of CTC from CRC at single-cell resolution holds the potential to provide key information for identification of prognostic and diagnostic markers as well as the development of precise and personalized cancer treatment.},
	language = {English},
	urldate = {2020-10-30},
	journal = {Frontiers in Pharmacology},
	author = {Tieng, Francis Yew Fu and Baharudin, Rashidah and Abu, Nadiah and Mohd Yunos, Ryia-Illani and Lee, Learn-Han and Ab Mutalib, Nurul-Syakima},
	year = {2020},
	note = {Publisher: Frontiers},
	keywords = {chemoresistance, Circulating Tumour Cells (CTC), colorectal cancer, metastasis, single-cell RNA sequencing, tumour heterogeneity},
	file = {Full Text PDF:/Users/yuliai/Zotero/storage/32LA47H3/Tieng et al. - 2020 - Single Cell Transcriptome in Colorectal Cancer—Cur.pdf:application/pdf},
}

@article{valdes-mora_single-cell_2018,
	title = {Single-{Cell} {Transcriptomics} in {Cancer} {Immunobiology}: {The} {Future} of {Precision} {Oncology}},
	volume = {9},
	issn = {1664-3224},
	shorttitle = {Single-{Cell} {Transcriptomics} in {Cancer} {Immunobiology}},
	url = {https://www.frontiersin.org/articles/10.3389/fimmu.2018.02582/full},
	doi = {10.3389/fimmu.2018.02582},
	abstract = {Cancer is a heterogeneous and complex disease. Tumours are formed by cancer cells and a myriad of non-cancerous cell types that together with the extracellular matrix form the tumour microenvironment. These cancer-associated cells and components contribute to shape the progression of cancer and are deeply involved in patient outcome. The immune system is an essential part of the tumour microenvironment, and induction of cancer immunotolerance is a necessary step involved in tumour formation and growth. Immune mechanisms are intimately associated with cancer progression, invasion, and metastasis; as well as to tumour dormancy and modulation of sensitivity to drug therapy. Transcriptome analyses have been extensively used to understand the heterogeneity of tumours, classifying tumours into molecular subtypes and establishing signatures that predict response to therapy and patient outcomes. However, the classification of the tumour cell diversity and specially the identification of rare populations has been limited in these transcriptomic analyses of bulk tumour cell populations. Massively-parallel single-cell RNAseq analysis has emerged as a powerful method to unravel heterogeneity and to study rare cell populations in cancer, through unsupervised sampling and modelling of transcriptional states in single cells. In this context, the study of the role of the immune system in cancer would benefit from single cell approaches, as it will enable the characterisation and/or discovery of the cell types and pathways involved in cancer immunotolerance otherwise missed in bulk transcriptomic information. Thus, the analysis of gene expression patterns at single cell resolution holds the potential to provide key information to develop precise and personalized cancer treatment including immunotherapy. This review is focused on the latest single-cell RNAseq methodologies able to agnostically study thousands of tumour cells as well as targeted single-cell RNAseq to study rare populations within tumours. In particular, we will discuss methods to study the immune system in cancer. We will also discuss the current challenges to the study of cancer at the single cell level and the potential solutions to the current approaches.},
	language = {English},
	urldate = {2020-10-30},
	journal = {Frontiers in Immunology},
	author = {Valdes-Mora, Fatima and Handler, Kristina and Law, Andrew M. K. and Salomon, Robert and Oakes, Samantha R. and Ormandy, Christopher J. and Gallego-Ortega, David},
	year = {2018},
	note = {Publisher: Frontiers},
	keywords = {tumour heterogeneity, MDSCs, ScRNA-seq, Single-cell transcriptomics, Stroma, tumour immunology},
	file = {Full Text PDF:/Users/yuliai/Zotero/storage/HTVST4C9/Valdes-Mora et al. - 2018 - Single-Cell Transcriptomics in Cancer Immunobiolog.pdf:application/pdf},
}

@article{charoentong_pan-cancer_2017,
	title = {Pan-cancer {Immunogenomic} {Analyses} {Reveal} {Genotype}-{Immunophenotype} {Relationships} and {Predictors} of {Response} to {Checkpoint} {Blockade}},
	volume = {18},
	issn = {2211-1247},
	doi = {10.1016/j.celrep.2016.12.019},
	abstract = {The Cancer Genome Atlas revealed the genomic landscapes of human cancers. In parallel, immunotherapy is transforming the treatment of advanced cancers. Unfortunately, the majority of patients do not respond to immunotherapy, making the identification of predictive markers and the mechanisms of resistance an area of intense research. To increase our understanding of tumor-immune cell interactions, we characterized the intratumoral immune landscapes and the cancer antigenomes from 20 solid cancers and created The Cancer Immunome Atlas (https://tcia.at/). Cellular characterization of the immune infiltrates showed that tumor genotypes determine immunophenotypes and tumor escape mechanisms. Using machine learning, we identified determinants of tumor immunogenicity and developed a scoring scheme for the quantification termed immunophenoscore. The immunophenoscore was a superior predictor of response to anti-cytotoxic T lymphocyte antigen-4 (CTLA-4) and anti-programmed cell death protein 1 (anti-PD-1) antibodies in two independent validation cohorts. Our findings and this resource may help inform cancer immunotherapy and facilitate the development of precision immuno-oncology.},
	language = {eng},
	number = {1},
	journal = {Cell Reports},
	author = {Charoentong, Pornpimol and Finotello, Francesca and Angelova, Mihaela and Mayer, Clemens and Efremova, Mirjana and Rieder, Dietmar and Hackl, Hubert and Trajanoski, Zlatko},
	year = {2017},
	pmid = {28052254},
	keywords = {Genomics, Humans, Neoplasms, Antigens, Neoplasm, Immunotherapy, Prognosis, Immunophenotyping, CD8-Positive T-Lymphocytes, cancer-germline antigens, neoantigens, predictive marker, tumor-infiltrating lymphocytes, CD4-Positive T-Lymphocytes, Cell Cycle Checkpoints, CTLA-4 Antigen, Genotype, Machine Learning, Mutation, Programmed Cell Death 1 Receptor},
	pages = {248--262},
	file = {Full Text:/Users/yuliai/Zotero/storage/46DJ878I/Charoentong et al. - 2017 - Pan-cancer Immunogenomic Analyses Reveal Genotype-.pdf:application/pdf},
}


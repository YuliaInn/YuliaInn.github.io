	pages = {428--439},
	file = {PubMed Central Full Text PDF:/Users/yuliai/Zotero/storage/XK7YQAAX/Scurr et al. - 2014 - Highly prevalent colorectal cancer-infiltrating LA.pdf:application/pdf},
}


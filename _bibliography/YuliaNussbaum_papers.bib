	keywords = {Immunotherapy, Colorectal cancer, Consensus molecular subtypes, Immune subtypes, Tumour microenviroment},
	pages = {118--129},
	file = {ScienceDirect Full Text PDF:/Users/yuliai/Zotero/storage/96PY864Z/Soldevilla et al. - 2019 - The correlation between immune subtypes and consen.pdf:application/pdf;ScienceDirect Snapshot:/Users/yuliai/Zotero/storage/E467KHTE/S0959804919307300.html:text/html},

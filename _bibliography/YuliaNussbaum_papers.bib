	pmid = {25853550},
	pmcid = {PMC4377852},
	file = {PubMed Central Full Text PDF:/Users/yuliai/Zotero/storage/2Q7M443H/Angelova et al. - 2015 - Characterization of the immunophenotypes and antig.pdf:application/pdf},
}


@article{ng_osteopontin_2015,
	title = {Osteopontin {Overexpression} {Induced} {Tumor} {Progression} and {Chemoresistance} to {Oxaliplatin} through {Induction} of {Stem}-{Like} {Properties} in {Human} {Colorectal} {Cancer}},

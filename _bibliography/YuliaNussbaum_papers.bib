	keywords = {Humans, Tumor Microenvironment, Colorectal Neoplasms, Adenoma, Carcinogenesis, Colorectum, Disease Progression, Interleukin-1beta, Interleukin-1β, Interleukin-6, Interlukin-6, RNA, Messenger, Stromal Cells},

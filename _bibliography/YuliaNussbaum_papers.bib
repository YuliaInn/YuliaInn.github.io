	pmid = {29655022},
	keywords = {Humans, Neoplasms, Animals, Immune System, Flow Cytometry, Phenotype, Single-Cell Analysis, Biomarkers, Epitopes, T-Lymphocyte, Lymphocytes, Myeloid Cells, T-Cell Antigen Receptor Specificity},
	pages = {187--196},
}


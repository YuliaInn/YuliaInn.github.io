@article{forrest_comparison_2014,
	title = {Comparison of visual and automated assessment of tumour inflammatory infiltrates in patients with colorectal cancer},
	volume = {50},
	issn = {0959-8049},
	url = {http://www.sciencedirect.com/science/article/pii/S0959804913009982},
	doi = {10.1016/j.ejca.2013.11.003},
	abstract = {Background
Cancer-associated inflammation is increasingly recognised to be an important determinant of oncological outcome. In colorectal cancer, the presence of peri-tumoural inflammatory/lymphocytic infiltrates predicts improved survival. To date, these infiltrates, assessed visually on haematoxylin and eosin (H\&E) stained sections, have failed to enter routine clinical practice, partly due to their subjective assessment and considerable inter-observer variation. The present study aims to develop an automated scoring method to enable consistent and reproducible assessment of tumour inflammatory infiltrates in colorectal cancer.
Methods
154 colorectal cancer patients who underwent curative resection were included in the study. The local inflammatory infiltrate was assessed using the method described by Klintrup–Makinen. H\&E tumour sections were uploaded to an image analysis programme (Slidepath, Leica Biosystems). An image analysis algorithm was developed to count the inflammatory cells at the invasive margin. The manual and automated assessments of the tumour inflammatory infiltrates were then compared.
Results
The automated inflammatory cell counts assessed using the freehand annotation method (p{\textless}0.001) and the rectangular box method (p{\textless}0.001) were significantly associated with both K–M score (p{\textless}0.001) and K–M grade (p{\textless}0.001). The inflammatory cell counts were divided using quartiles to group tumours with similar inflammatory cell densities. There was good agreement between the manual and automated scoring methods (intraclass correlation coefficient (ICC)=0.82). Similar to the visual K–M scoring system, the automated K–M classification of the inflammatory cell counts, using quartiles, was significantly associated with venous invasion (p{\textless}0.05) and modified Glasgow Prognostic Score (mGPS) (p⩽0.05). On univariate survival analysis, both automated K–M category (p{\textless}0.05) and automated K–M grade (p{\textless}0.005) were associated with cancer-specific survival.
Conclusion
The results of the present study demonstrate that automated assessment effectively recapitulates the clinical value of visual assessment of the local inflammatory cell infiltrate at the invasive margin of colorectal tumours. In addition, it is possible to obtain an objective assessment of tumour inflammatory infiltrates using routinely stained H\&E sections. An automated, computer-based scoring method is therefore a workable and cost-effective approach to clinical assessment of local immune cell infiltrates in colorectal cancer.},
	language = {en},
	number = {3},
	urldate = {2020-10-15},
	journal = {European Journal of Cancer},
	author = {Forrest, R. and Guthrie, G. J. K. and Orange, C. and Horgan, P. G. and McMillan, D. C. and Roxburgh, C. S. D.},
	month = feb,
	year = {2014},
	keywords = {Colorectal cancer, Automated assessment, Tumour inflammatory cell infiltrate},
	pages = {544--552},
	file = {ScienceDirect Full Text PDF:/Users/yuliai/Zotero/storage/SLQ4BKHV/Forrest et al. - 2014 - Comparison of visual and automated assessment of t.pdf:application/pdf;ScienceDirect Snapshot:/Users/yuliai/Zotero/storage/Z4HXQACZ/S0959804913009982.html:text/html},
}


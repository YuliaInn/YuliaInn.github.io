	journal = {Bioinformatics},
	author = {Reid, John E. and Wernisch, Lorenz},
	month = oct,
	year = {2016},
	pmid = {27318198},
	pmcid = {PMC5039927},
	pages = {2973--2980},
	file = {PubMed Central Full Text PDF:/Users/yuliai/Zotero/storage/SDUHLI7E/Reid and Wernisch - 2016 - Pseudotime estimation deconfounding single cell t.pdf:application/pdf},
}

@article{di_phenotype_2020,
	title = {Phenotype molding of {T} cells in colorectal cancer by single-cell analysis},
	volume = {146},
	copyright = {© 2020 UICC},

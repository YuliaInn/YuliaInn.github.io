Section: Imaging, Diagnosis, Prognosis},
	pages = {1891--1899},
	file = {Full Text PDF:/Users/yuliai/Zotero/storage/HCETD3D5/Anitei et al. - 2014 - Prognostic and Predictive Values of the Immunoscor.pdf:application/pdf;Snapshot:/Users/yuliai/Zotero/storage/9V8G8V3S/1891.html:text/html},
}


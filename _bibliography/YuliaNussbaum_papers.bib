	journal = {Journal of Translational Medicine},
	author = {Benvenuto, Monica and Sileri, Pierpaolo and Rossi, Piero and Masuelli, Laura and Fantini, Massimo and Nanni, Monica and Franceschilli, Luana and Sconocchia, Giuseppe and Lanzilli, Giulia and Arriga, Roberto and Faggioni, Giovanni and Lista, Florigio and Orlandi, Augusto and Manzari, Vittorio and Gaspari, Achille Lucio and Modesti, Andrea and Bei, Roberto},

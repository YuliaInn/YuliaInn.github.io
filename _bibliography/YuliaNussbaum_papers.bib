	year = {2020},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/ijc.32856},
	keywords = {colorectal cancer, immunosuppression, single-cell mass cytometry, T-cell exhaustion, T-cell phenotype},
	pages = {2281--2295},
	file = {Full Text PDF:/Users/yuliai/Zotero/storage/GQXCQH3R/Di et al. - 2020 - Phenotype molding of T cells in colorectal cancer .pdf:application/pdf;Snapshot:/Users/yuliai/Zotero/storage/YAKZVM7P/ijc.html:text/html},

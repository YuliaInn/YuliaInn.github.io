	journal = {Methods},
	author = {Stack, Edward C. and Wang, Chichung and Roman, Kristin A. and Hoyt, Clifford C.},
	month = nov,
	year = {2014},
	keywords = {Biomarkers, Cancer, Multiplexed, Multispectral, Pathology, Quantitative, TSA},
	pages = {46--58},
	file = {ScienceDirect Full Text PDF:/Users/yuliai/Zotero/storage/5Q82F4QK/Stack et al. - 2014 - Multiplexed immunohistochemistry, imaging, and qua.pdf:application/pdf;ScienceDirect Snapshot:/Users/yuliai/Zotero/storage/7ZGLEQUI/S1046202314002837.html:text/html},

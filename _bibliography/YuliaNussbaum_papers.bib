	keywords = {Humans, Colorectal Neoplasms, Tissue Array Analysis, Biomarkers, Tumor, CDX2 Transcription Factor, Fluorescent Antibody Technique, Image Interpretation, Computer-Assisted, SOXB1 Transcription Factors},
	pages = {120--134},

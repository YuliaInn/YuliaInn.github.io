	pmid = {28531945},
	keywords = {Humans, Animals, Mice, Prognosis, Colorectal cancer, Colorectal Neoplasms, Cell Line, Tumor, Cell Proliferation, Mice, Inbred BALB C, Male, Cell Movement, Down-Regulation, Metastasis, Bioinformatics, RNA, Messenger, Osteopontin, Apoptosis, Cadherins, Cell Cycle, Computational Biology, Epithelial-Mesenchymal Transition, Epithelial-mesenchymal-transition, HCT116 Cells, Mice, Nude, RNA, Small Interfering, Secreted phosphoprotein 1, Up-Regulation, Vimentin},

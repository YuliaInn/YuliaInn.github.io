	year = {2018},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/jcmm.13686},
	keywords = {survival, colorectal cancer, connectivity mapping, Osteopontin},
	pages = {4097--4105},
	file = {Full Text PDF:/Users/yuliai/Zotero/storage/E8AAR9YI/Wei et al. - 2018 - In vitro and clinical data analysis of Osteopontin.pdf:application/pdf;Snapshot:/Users/yuliai/Zotero/storage/ZE6KGSIM/jcmm.html:text/html},

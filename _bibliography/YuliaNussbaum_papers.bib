	year = {2018},
	pmid = {29704502},
	keywords = {Humans, Prognosis, Colorectal cancer, Colorectal Neoplasms, Gene Expression Regulation, Neoplastic, Neoplasm Metastasis, Neoplasm Proteins, Biomarkers, Tumor, Signal Transduction, Bioinformatics, Gene Regulatory Networks, Metastasis-free survival rate, Organelles, Survival time},
	pages = {113--118},
}

@article{laughney_regenerative_2020,
	title = {Regenerative lineages and immune-mediated pruning in lung cancer metastasis},
	volume = {26},
	copyright = {2020 The Author(s), under exclusive licence to Springer Nature America, Inc.},

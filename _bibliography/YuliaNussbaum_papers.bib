	urldate = {2020-10-28},
	journal = {ESMO Open},
	author = {Smeby, Jørgen and Sveen, Anita and Bergsland, Christian H and Eilertsen, Ina A and Danielsen, Stine A and Eide, Peter W and Hektoen, Merete and Guren, Marianne G and Nesbakken, Arild and Bruun, Jarle and Lothe, Ragnhild A},
	month = jun,
